<header>
    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper blue darken-2 nav-z-index">
                <a href="#" class="brand-logo">Harley weather</a>
                <a href="#" data-activates="slide-out" class="button-collapse show-on-large right">
                    <i class="material-icons">menu</i>
                </a>
            </div>
        </nav>
    </div>
    <ul id="slide-out" class="side-nav">
        <div class="container">
            <li>
                <h3 class="center-align">Menu</h3>
            </li>
            <li>
                <label>Select city:</label>
                <select class="browser-default" name="cities"></select>
            </li>
            <li class="params"></li>
            <li>
                <div class="row">
                    <h5 class="center-align title">SELECT PERIOD</h5>
                    <form class="col s12 m12 l12">
                        <p class="left-align">From</p>
                        <input type="date" name="date-from" class="datepicker date-size">
                    </form>
                    <form class="col s12 m12 l12">
                        <p class="left-align">to</p>
                        <input type="date" name="date-to" class="datepicker date-size">
                    </form>
                </div>
                <a class="waves-effect waves-light btn blue" id="statistics">Show</a>
            </li>
        </div>
    </ul>
</header>
<main>
    <div class="container">
        <div class="row">
            <div class="content"></div>
            <div class="col s12">
                <div id="map" class="map card-panel z-depth-1 hoverable"></div>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m12 l12">
                <div class="card-panel z-depth-1 hoverable chart-current-weather">
                    <div class="section">
                        <div class="row">
                            <div class="col l6 m6 s6 left">
                                <h5>CURRENT WEATHER COMPARE SERVICES</h5>
                            </div>
                            <div class="col l3 m3 s3 right">
                                <label>CITY:</label>
                                <select class="browser-default cur-params" name="city"></select>
                            </div>
                            <div class="col l3 m3 s3 right">
                                <label>PARAMETER:</label>
                                <select class="browser-default cur-params" name="param"></select>
                            </div>
                        </div>
                        <div class="divider"></div>
                    </div>
                    <canvas id="chart-current-weather"></canvas>
                </div>
            </div>
        </div>
    </div>
</main>